generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id
  firstName    String?
  lastName     String?
  username     String?
  phoneNumber  String?
  licenseToken String?
  paid         Boolean? @default(false)
}
